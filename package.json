{
  "name": "kitweaver-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cd apps/web && pnpm dev",
    "build": "cd apps/web && pnpm build",
    "start": "cd apps/web && pnpm start",
    "lint": "cd apps/web && pnpm lint",
    "db:push": "cd apps/web && npx prisma db push",
    "db:studio": "cd apps/web && npx prisma studio",
    "seed": "cd apps/web && npx tsx prisma/seed.ts"
  },
  "devDependencies": {
    "prettier": "latest"
  },
  "packageManager": "pnpm@9.0.0"
}
